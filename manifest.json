{
  "name": "Average Calculator",
  "version": "1.0",
  "description": "Calculates your average marks",
  "content_scripts": [
    {
      "matches": [
        "https://intranet.iut.u-bordeaux.fr/notes"
      ],
      "js": ["content.js"]
    }
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Click here to open the menu",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}